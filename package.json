    "@types/webpack": "^4.1.4",
    "@types/webpack-dev-middleware": "^2.0.1",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.3.0",
    "express": "^4.16.3",
    "glob": "^7.1.2",
    "mocha": "^5.1.1",
    "mocha-chrome": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.2",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
