{
  "name": "isomorphic-xml2js",
  "version": "0.0.3",
  "description": "XML parsing for browser and node.js",
  "main": "dist/lib/index.js",
  "browser": "dist/lib/browser.js",
  "types": "typings/lib/index.js",
  "scripts": {
    "build": "run-p build:nodejs build:browser",
    "build:nodejs": "tsc",
